{
    "name": "matthiasnoback/sphinx-documentation-collector",
    "description": "A tool for collecting documentation roots from different libraries ",
    "license": "MIT",
    "type": "library",
    "keywords": ["Sphinx", "documentation"],
    "homepage": "http://github.com/matthiasnoback/sphinx-documentation-collector",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "require": {
        "symfony/finder": "2.*",
        "symfony/filesystem": "2.*",
        "aura/cli": "2.*@dev",
        "pimple/pimple": "1.*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=3.7"
    },
    "autoload": {
        "psr-0": {
            "Matthias\\SphinxDocumentation\\": "src/",
            "Matthias\\SphinxDocumentation\\Tests\\": "tests/"
        }
    }
}
